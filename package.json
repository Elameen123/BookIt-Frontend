{
  "name": "paubookit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/dom": "^10.4.0",
    "axios": "^1.11.0",
    "craco-alias": "^3.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
  "start": "set NODE_OPTIONS=--openssl-legacy-provider && craco start",
